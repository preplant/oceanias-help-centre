{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#BF00FF",
    "light": "#BF00FF",
    "dark": "#BF00FF"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
